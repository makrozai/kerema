<!DOCTYPE html>
html(lang="es")
  head
    meta(charset="UTF-8")
    title Documents
    link(rel="stylesheet", href="dist/css/app.css")
  body
    include components/navbar-1.pug
    main.c-banner-products
      img(src="dist/img/banner-work.png", alt="")
      .c-banner-products__title
        h1.wow.fadeInDown nuestro<br>trabajo


    section.c-presentations.c-contact-back
      img(src="dist/img/contact-back.png", alt="")
      .container
        .row
          .col-12
            .c-contact
              .c-contact__description.wow.fadeInLeft
                img(src="dist/img/title-contact.png", alt="")
                h3 Informacion<br>de contacto
                p 983 467 917
                p 974 891 025
                p info@keremaperu.com
                h3 direcciÃ³n
                p Carretera el Oconal - Villa Rica (0.2 km)
                h3 Horario
                p 09:00 A 19:00
              .c-contact__form.wow.fadeInRight
                form(action="#!")
                  .c-contact__form-group
                    img(src="dist/img/contact-form-title.png", alt="")
                  .c-contact__form-group
                    label
                      span Tu nombre
                      input(type="text")
                  .c-contact__form-group
                    label
                      span E-mail
                      input(type="email")
                  .c-contact__form-group
                    label
                      span Asunto
                      input(type="text")
                  .c-contact__form-group
                    label
                      span Mensaje
                      textarea(cols="30", rows="10")
                  .c-contact__form-group
                    button enviar >
            a(href="#!").c-presentations__button regresar
            
    include components/section-clients.pug
